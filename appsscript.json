{
  "timeZone": "Asia/Kolkata",
  "dependencies": {
  },
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "oauthScopes":[
    "https://www.googleapis.com/auth/workspace.linkpreview",
    "https://www.googleapis.com/auth/script.external_request",
    "https://www.googleapis.com/auth/workspace.linkcreate"
  ],
  "urlFetchWhitelist": [
    "https://kg.artsdata.ca/"
  ],
  "addOns":{
    "common": {
      "name": "Artsdata Preview Add-on",
      "logoUrl": "https://kg.artsdata.ca/assets/artsdata_logo_circle-b81a55f1319c2ac8759c437d5fc4aaebc259f5f7bd81d9c70fbd74d4f43bef60.png"
    },
    "docs": {
      "linkPreviewTriggers":[
        {
          "runFunction": "artsdataPreview",
          "patterns": [
            {
              "hostPattern":"*artsdata*"
            }
          ],
          "labelText":"Artsdata-entity"
        }
      ]
    },
    "sheets": {
      "linkPreviewTriggers":[
        {
          "runFunction": "artsdataPreview",
          "patterns": [
            {
              "hostPattern":"*artsdata*"
            }
          ],
          "labelText":"Artsdata-entity"
        }
      ]
    },
    "slides": {
      "linkPreviewTriggers":[
        {
          "runFunction": "artsdataPreview",
          "patterns": [
            {
              "hostPattern":"*artsdata*"
            }
          ],
          "labelText":"Artsdata-entity"
        }
      ]
    }
  }
}