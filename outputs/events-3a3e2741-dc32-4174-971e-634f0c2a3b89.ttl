<http://indigenous-performances/row/temporary/8ff6f144ed9844e88f04796fb797d5c9> a
    schema:Event;
  schema:name "Twin Flames @ Twin Flames Live in Sarnia";
  schema:additionalType <http://kg.artsdata.ca/resource/MusicPerformance>;
  schema:startDate "2024-10-03T20:00:00-04:00"^^xsd:dateTime;
