<http://indigenous-performances/row/temporary/dbbb44ed8879450ab0b41257ceaa74de> a
    schema:Event;
  schema:name "Snotty Nose Rez Kids - RED FUTURE Tour";
  schema:additionalType <http://kg.artsdata.ca/resource/MusicPerformance>;
  schema:startDate "2024-10-30T19:30:00-06:00"^^xsd:dateTime;
  schema:url <https://tickets.keycitytheatre.com/TheatreManager/1/login?event=1531>;
