<http://indigenous-performances/row/temporary/409745eca8184e37830d51d82158867d> a
    schema:Event;
  schema:name "Jeremy Dutcher";
  schema:additionalType <http://kg.artsdata.ca/resource/MusicPerformance>;
  schema:startDate "2025-02-11T18:30:00-07:00"^^xsd:dateTime;
  schema:url <https://www.bandsintown.com/e/106119375?affil_code=js_jeremydutcher.com&app_id=js_jeremydutcher.com&came_from=242&utm_campaign=event&utm_medium=web&utm_source=widget>;
